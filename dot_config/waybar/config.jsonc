{
	"layer": "top",
	"position": "top",
	"height": 15,
	"spacing": 4,
	"margin-top": 5,
	"margin-bottom": 10,
	"modules-center": ["sway/workspaces", "network", "cpu", "memory", "temperature", "battery", "battery#bat2", "clock"],
	"sway/workspaces": {
		"disable-scroll": true,
		"all-outputs": false,
		"format": "{icon}",
		"format-icons": {
			"default": "/",
			"urgent": "!",
			"focused": "/"
		}
	},
	"clock": {
		"format": "<span foreground='#C6AAE8'> </span>{:%a %d %H:%M}"
	}
}

